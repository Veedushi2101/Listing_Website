<% if(successMsg && successMsg.length){%>
    <div class="alert alert-success alert-dismissible fade show col-3 offset-8 mt-3" role="alert">
      <%= successMsg %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<% } %>

<% if(errorMsg && errorMsg.length){%>
    <div class="alert alert-danger alert-dismissible fade show col-3 offset-8 mt-3" role="alert">
      <%= errorMsg %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<% } %>
